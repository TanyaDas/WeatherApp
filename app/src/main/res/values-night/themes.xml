<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.OneTech" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_light</item>
        <item name="colorPrimaryVariant">@color/primary</item>
        <item name="colorOnPrimary">@color/colorBlack</item>
        <item name="colorOnSurface">@color/primary_light</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondary</item>
        <item name="colorSecondaryVariant">@color/secondary_light</item>
        <item name="colorOnSecondary">@color/colorWhite</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <item name="android:fontFamily" >@font/poppins_medium</item>
    </style>
</resources>